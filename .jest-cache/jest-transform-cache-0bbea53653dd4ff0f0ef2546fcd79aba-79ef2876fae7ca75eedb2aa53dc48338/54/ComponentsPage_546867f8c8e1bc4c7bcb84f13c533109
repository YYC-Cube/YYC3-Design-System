/*  Section Component  */
function Section({
  title,
  id,
  children,
  jsxCode,
  a11yRole,
  a11yKeys,
  a11yAttrs,
  tokens,
  onSelectA11y
}) {
  /* istanbul ignore next */
  cov_c8twfia8v().f[9]++;
  const [copied, setCopied] =
  /* istanbul ignore next */
  (cov_c8twfia8v().s[66]++, (0, react_1.useState)(false));
  const {
    t
  } =
  /* istanbul ignore next */
  (cov_c8twfia8v().s[67]++, (0, LanguageContext_1.useLanguage)());
  const handleCopyJsx =
  /* istanbul ignore next */
  (cov_c8twfia8v().s[68]++, (0, react_1.useCallback)(async () => {
    /* istanbul ignore next */
    cov_c8twfia8v().f[10]++;
    cov_c8twfia8v().s[69]++;
    if (!jsxCode) {
      /* istanbul ignore next */
      cov_c8twfia8v().b[17][0]++;
      cov_c8twfia8v().s[70]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_c8twfia8v().b[17][1]++;
    }
    cov_c8twfia8v().s[71]++;
    try {
      /* istanbul ignore next */
      cov_c8twfia8v().s[72]++;
      await navigator.clipboard.writeText(jsxCode);
      /* istanbul ignore next */
      cov_c8twfia8v().s[73]++;
      setCopied(true);
      /* istanbul ignore next */
      cov_c8twfia8v().s[74]++;
      sonner_1.toast.success(`${title} JSX ${t('components.copiedJsx')}`);
      /* istanbul ignore next */
      cov_c8twfia8v().s[75]++;
      setTimeout(() => {
        /* istanbul ignore next */
        cov_c8twfia8v().f[11]++;
        cov_c8twfia8v().s[76]++;
        return setCopied(false);
      }, 2000);
    } catch {
      /* istanbul ignore next */
      cov_c8twfia8v().s[77]++;
      sonner_1.toast.error(t('common.error'));
    }
  }, [jsxCode, title, t]));
  /* istanbul ignore next */
  cov_c8twfia8v().s[78]++;
  return (0, jsx_runtime_1.jsxs)("section", {
    id: id,
    className: "scroll-mt-20",
    children: [(0, jsx_runtime_1.jsxs)("div", {
