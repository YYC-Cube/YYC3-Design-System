        style: {
          fontSize: '1.125rem'
        },
        children: [(0, jsx_runtime_1.jsx)("span", {
          className: "size-6 rounded flex items-center justify-center text-xs",
          style: {
            background: 'color-mix(in oklch, var(--primary) 12%, var(--card))',
            color: 'var(--primary)',
            fontWeight: 700
          },
          "aria-hidden": "true",
          children: "#"
        }), title]
      }), (0, jsx_runtime_1.jsxs)("div", {
        className: "flex items-center gap-1.5",
        children: [
        /* istanbul ignore next */
        (cov_c8twfia8v().b[18][0]++, a11yRole) &&
        /* istanbul ignore next */
        (cov_c8twfia8v().b[18][1]++, onSelectA11y) &&
        /* istanbul ignore next */
        (cov_c8twfia8v().b[18][2]++, (0, jsx_runtime_1.jsxs)("button", {
          onClick: () => {
            /* istanbul ignore next */
            cov_c8twfia8v().f[12]++;
            cov_c8twfia8v().s[79]++;
            return onSelectA11y({
              title,
              role: a11yRole,
              keys: 
              /* istanbul ignore next */
              (cov_c8twfia8v().b[19][0]++, a11yKeys) ??
              /* istanbul ignore next */
              (cov_c8twfia8v().b[19][1]++, ''),
              attrs: 
              /* istanbul ignore next */
              (cov_c8twfia8v().b[20][0]++, a11yAttrs) ??
              /* istanbul ignore next */
              (cov_c8twfia8v().b[20][1]++, ''),
              tokens: 
              /* istanbul ignore next */
              (cov_c8twfia8v().b[21][0]++, tokens) ??
              /* istanbul ignore next */
              (cov_c8twfia8v().b[21][1]++, '')
            });
          },
          className: "flex items-center gap-1 px-2 py-1 rounded-md text-xs transition-all focus-visible:outline-none",
          style: {
            border: '1px solid var(--border)',
            background: 'var(--card)',
            color: 'var(--muted-foreground)',
            cursor: 'pointer',
            outline: '2px solid transparent',
            transition: `all var(--duration-fast) var(--easing-out)`
          },
          onMouseEnter: e => {
            /* istanbul ignore next */
            cov_c8twfia8v().f[13]++;
            cov_c8twfia8v().s[80]++;
            e.currentTarget.style.background = 'var(--muted)';
          },
          onMouseLeave: e => {
            /* istanbul ignore next */
            cov_c8twfia8v().f[14]++;
            cov_c8twfia8v().s[81]++;
            e.currentTarget.style.background = 'var(--card)';
          },
          onFocus: e => {
            /* istanbul ignore next */
            cov_c8twfia8v().f[15]++;
            cov_c8twfia8v().s[82]++;
            e.currentTarget.style.outline = `2px solid var(--ring)`;
          },
          onBlur: e => {
            /* istanbul ignore next */
            cov_c8twfia8v().f[16]++;
            cov_c8twfia8v().s[83]++;
            e.currentTarget.style.outline = '2px solid transparent';
          },
          "aria-label": `${t('components.a11yInfo')}  ${title}`,
          children: [(0, jsx_runtime_1.jsx)(lucide_react_1.Eye, {
            className: "size-3",
            "aria-hidden": "true"
          }), (0, jsx_runtime_1.jsx)("span", {
            className: "hidden sm:inline",
            children: t('components.a11yInfo')
          })]
        })), 
        /* istanbul ignore next */
        (cov_c8twfia8v().b[22][0]++, jsxCode) &&
        /* istanbul ignore next */
        (cov_c8twfia8v().b[22][1]++, (0, jsx_runtime_1.jsxs)("button", {
          onClick: handleCopyJsx,
          className: "flex items-center gap-1 px-2 py-1 rounded-md text-xs transition-all focus-visible:outline-none",
          style: {
            border: `1px solid ${copied ?
            /* istanbul ignore next */
            (cov_c8twfia8v().b[23][0]++, 'var(--success)') :
            /* istanbul ignore next */
            (cov_c8twfia8v().b[23][1]++, 'var(--border)')}`,
            background: copied ?
            /* istanbul ignore next */
            (cov_c8twfia8v().b[24][0]++, 'color-mix(in oklch, var(--success) 10%, var(--card))') :
            /* istanbul ignore next */
            (cov_c8twfia8v().b[24][1]++, 'var(--card)'),
            color: copied ?
            /* istanbul ignore next */
            (cov_c8twfia8v().b[25][0]++, 'var(--success)') :
            /* istanbul ignore next */
            (cov_c8twfia8v().b[25][1]++, 'var(--muted-foreground)'),
            cursor: 'pointer',
            outline: '2px solid transparent',
            transition: `all var(--duration-fast) var(--easing-out)`
          },
          onMouseEnter: e => {
            /* istanbul ignore next */
            cov_c8twfia8v().f[17]++;
            cov_c8twfia8v().s[84]++;
            if (!copied) {
              /* istanbul ignore next */
              cov_c8twfia8v().b[26][0]++;
              cov_c8twfia8v().s[85]++;
              e.currentTarget.style.background = 'var(--muted)';
            } else
            /* istanbul ignore next */
            {
              cov_c8twfia8v().b[26][1]++;
            }
          },
          onMouseLeave: e => {
            /* istanbul ignore next */
            cov_c8twfia8v().f[18]++;
            cov_c8twfia8v().s[86]++;
            if (!copied) {
              /* istanbul ignore next */
              cov_c8twfia8v().b[27][0]++;
              cov_c8twfia8v().s[87]++;
              e.currentTarget.style.background = 'var(--card)';
            } else
            /* istanbul ignore next */
            {
              cov_c8twfia8v().b[27][1]++;
            }
          },
          onFocus: e => {
            /* istanbul ignore next */
            cov_c8twfia8v().f[19]++;
            cov_c8twfia8v().s[88]++;
            e.currentTarget.style.outline = `2px solid var(--ring)`;
          },
          onBlur: e => {
            /* istanbul ignore next */
            cov_c8twfia8v().f[20]++;
            cov_c8twfia8v().s[89]++;
            e.currentTarget.style.outline = '2px solid transparent';
          },
          "aria-label": `${t('components.copyJsx')}  ${title}`,
          children: [copied ?
          /* istanbul ignore next */
          (cov_c8twfia8v().b[28][0]++, (0, jsx_runtime_1.jsx)(lucide_react_1.CheckCircle2, {
            className: "size-3",
            "aria-hidden": "true"
          })) :
          /* istanbul ignore next */
          (cov_c8twfia8v().b[28][1]++, (0, jsx_runtime_1.jsx)(lucide_react_1.Copy, {
            className: "size-3",
            "aria-hidden": "true"
          })), (0, jsx_runtime_1.jsx)("span", {
            className: "hidden sm:inline",
            children: copied ?
            /* istanbul ignore next */
            (cov_c8twfia8v().b[29][0]++, t('components.copiedJsx')) :
            /* istanbul ignore next */
            (cov_c8twfia8v().b[29][1]++, t('components.copyJsx'))
          })]
        }))]
      })]
    }), (0, jsx_runtime_1.jsx)("div", {
      className: "rounded-xl p-4 sm:p-6",
      style: {
        border: '1px solid var(--border)',
        background: 'var(--card)'
      },
      children: children
    })]
  });
}
/*  A11y Info Panel  */
function A11yPanel({
  info,
  onClose
}) {
  /* istanbul ignore next */
  cov_c8twfia8v().f[21]++;
  const {
    t
  } =
  /* istanbul ignore next */
  (cov_c8twfia8v().s[90]++, (0, LanguageContext_1.useLanguage)());
  /* istanbul ignore next */
  cov_c8twfia8v().s[91]++;
  if (!info) {
    /* istanbul ignore next */
    cov_c8twfia8v().b[30][0]++;
    cov_c8twfia8v().s[92]++;
    return null;
  } else
  /* istanbul ignore next */
  {
    cov_c8twfia8v().b[30][1]++;
  }
  cov_c8twfia8v().s[93]++;
