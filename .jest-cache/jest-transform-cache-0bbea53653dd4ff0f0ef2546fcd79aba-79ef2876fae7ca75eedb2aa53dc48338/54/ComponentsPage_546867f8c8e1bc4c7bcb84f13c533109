function PaginationDemo() {
  /* istanbul ignore next */
  cov_c8twfia8v().f[55]++;
  const [page, setPage] =
  /* istanbul ignore next */
  (cov_c8twfia8v().s[148]++, (0, react_1.useState)(1));
  const total =
  /* istanbul ignore next */
  (cov_c8twfia8v().s[149]++, 10);
  const {
    t
  } =
  /* istanbul ignore next */
  (cov_c8twfia8v().s[150]++, (0, LanguageContext_1.useLanguage)());
  /* istanbul ignore next */
  cov_c8twfia8v().s[151]++;
  return (0, jsx_runtime_1.jsxs)("nav", {
    "aria-label": t('components.pagination'),
    className: "flex items-center gap-1",
    children: [(0, jsx_runtime_1.jsx)(button_1.Button, {
      variant: "outline",
      size: "sm",
      disabled: page === 1,
      onClick: () => {
        /* istanbul ignore next */
        cov_c8twfia8v().f[56]++;
        cov_c8twfia8v().s[152]++;
        return setPage(page - 1);
      },
      "aria-label": t('components.prevPage'),
      children: t('components.prevPage')
    }), Array.from({
      length: Math.min(5, total)
    }, (_, i) => {
      /* istanbul ignore next */
      cov_c8twfia8v().f[57]++;
      const p =
      /* istanbul ignore next */
      (cov_c8twfia8v().s[153]++, page <= 3 ?
      /* istanbul ignore next */
      (cov_c8twfia8v().b[38][0]++, i + 1) :
      /* istanbul ignore next */
      (cov_c8twfia8v().b[38][1]++, page >= total - 2 ?
      /* istanbul ignore next */
      (cov_c8twfia8v().b[39][0]++, total - 4 + i) :
      /* istanbul ignore next */
      (cov_c8twfia8v().b[39][1]++, page - 2 + i)));
      /* istanbul ignore next */
      cov_c8twfia8v().s[154]++;
      return (0, jsx_runtime_1.jsx)(button_1.Button, {
        variant: p === page ?
        /* istanbul ignore next */
        (cov_c8twfia8v().b[40][0]++, 'default') :
        /* istanbul ignore next */
        (cov_c8twfia8v().b[40][1]++, 'outline'),
        size: "sm",
        onClick: () => {
          /* istanbul ignore next */
          cov_c8twfia8v().f[58]++;
          cov_c8twfia8v().s[155]++;
          return setPage(p);
        },
        "aria-label": `${t('components.pagePrefix')}${p}${t('components.pageSuffix')}`,
        "aria-current": p === page ?
        /* istanbul ignore next */
        (cov_c8twfia8v().b[41][0]++, 'page') :
        /* istanbul ignore next */
        (cov_c8twfia8v().b[41][1]++, undefined),
        className: "w-9",
        children: p
      }, p);
    }), (0, jsx_runtime_1.jsx)(button_1.Button, {
      variant: "outline",
      size: "sm",
      disabled: page === total,
      onClick: () => {
        /* istanbul ignore next */
        cov_c8twfia8v().f[59]++;
        cov_c8twfia8v().s[156]++;
        return setPage(page + 1);
      },
      "aria-label": t('components.nextPage'),
      children: t('components.nextPage')
    })]
  });
}
