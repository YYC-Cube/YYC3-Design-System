const token_utils_1 = require("./token-utils");
const mockTokens = {
    color: {
        primary: '#d45a5f',
        secondary: '#3d4a5c',
        background: '#ffffff',
        foreground: '#000000',
    },
    spacing: {
        xs: '4px',
        sm: '8px',
        md: '16px',
        lg: '24px',
        xl: '32px',
    },
    radius: {
        sm: '4px',
        md: '8px',
        lg: '12px',
    },
    font: {
        size: {
            sm: '14px',
            base: '16px',
            lg: '18px',
        },
    },
};
describe('token-utils', () => {
    describe('getTokenPath', () => {
        it('应该将字符串路径转换为数组', () => {
            expect((0, token_utils_1.getTokenPath)('color.primary')).toEqual(['color', 'primary']);
        });
        it('应该保持数组路径不变', () => {
            expect((0, token_utils_1.getTokenPath)(['color', 'primary'])).toEqual(['color', 'primary']);
        });
        it('应该处理空路径', () => {
            expect((0, token_utils_1.getTokenPath)('')).toEqual(['']);
        });
    });
    describe('getNestedValue', () => {
        it('应该获取嵌套值', () => {
            expect((0, token_utils_1.getNestedValue)(mockTokens, 'color.primary')).toBe('#d45a5f');
        });
        it('应该获取深层嵌套值', () => {
            expect((0, token_utils_1.getNestedValue)(mockTokens, 'font.size.base')).toBe('16px');
        });
        it('应该处理数组路径', () => {
            expect((0, token_utils_1.getNestedValue)(mockTokens, ['color', 'secondary'])).toBe('#3d4a5c');
        });
        it('应该对不存在的路径返回undefined', () => {
            expect((0, token_utils_1.getNestedValue)(mockTokens, 'color.nonexistent')).toBeUndefined();
        });
        it('应该对无效路径返回undefined', () => {
            expect((0, token_utils_1.getNestedValue)(mockTokens, 'invalid.path')).toBeUndefined();
        });
    });
    describe('hasToken', () => {
        it('应该对存在的token返回true', () => {
            expect((0, token_utils_1.hasToken)(mockTokens, 'color.primary')).toBe(true);
        });
        it('应该对不存在的token返回false', () => {
            expect((0, token_utils_1.hasToken)(mockTokens, 'color.nonexistent')).toBe(false);
        });
        it('应该对深层嵌套的token返回true', () => {
            expect((0, token_utils_1.hasToken)(mockTokens, 'font.size.base')).toBe(true);
        });
    });
    describe('isTokenColor', () => {
        it('应该识别HEX颜色', () => {
            expect((0, token_utils_1.isTokenColor)(mockTokens, 'color.primary')).toBe(true);
        });
        it('应该识别6位HEX颜色', () => {
            expect((0, token_utils_1.isTokenColor)(mockTokens, 'color.secondary')).toBe(true);
        });
        it('应该对非颜色返回false', () => {
            expect((0, token_utils_1.isTokenColor)(mockTokens, 'spacing.xs')).toBe(false);
        });
        it('应该对不存在的路径返回false', () => {
            expect((0, token_utils_1.isTokenColor)(mockTokens, 'color.nonexistent')).toBe(false);
        });
    });
    describe('isTokenSize', () => {
        it('应该识别px尺寸', () => {
            expect((0, token_utils_1.isTokenSize)(mockTokens, 'spacing.xs')).toBe(true);
        });
        it('应该识别rem尺寸', () => {
            const tokens = { size: '1rem' };
            expect((0, token_utils_1.isTokenSize)(tokens, 'size')).toBe(true);
        });
        it('应该识别数字尺寸', () => {
            const tokens = { size: '16' };
            expect((0, token_utils_1.isTokenSize)(tokens, 'size')).toBe(true);
        });
        it('应该对非尺寸返回false', () => {
            expect((0, token_utils_1.isTokenSize)(mockTokens, 'color.primary')).toBe(false);
        });
    });
    describe('isTokenSpacing', () => {
        it('应该识别px间距', () => {
            expect((0, token_utils_1.isTokenSpacing)(mockTokens, 'spacing.xs')).toBe(true);
        });
        it('应该识别rem间距', () => {
            const tokens = { spacing: '1rem 2rem' };
            expect((0, token_utils_1.isTokenSpacing)(tokens, 'spacing')).toBe(true);
        });
        it('应该对非间距返回false', () => {
            expect((0, token_utils_1.isTokenSpacing)(mockTokens, 'color.primary')).toBe(false);
        });
    });
    describe('createTokenAccessor', () => {
        it('应该创建token访问器', () => {
            const accessor = (0, token_utils_1.createTokenAccessor)(mockTokens);
            expect(accessor.get('color')).toEqual(mockTokens.color);
        });
        it('应该支持二级访问', () => {
            const accessor = (0, token_utils_1.createTokenAccessor)(mockTokens);
            expect(accessor.get2('color', 'primary')).toBe('#d45a5f');
        });
        it('应该支持三级访问', () => {
            const accessor = (0, token_utils_1.createTokenAccessor)(mockTokens);
            expect(accessor.get3('font', 'size', 'base')).toBe('16px');
        });
    });
    describe('createTokenValidator', () => {
        it('应该创建token验证器', () => {
            const validator = (0, token_utils_1.createTokenValidator)(mockTokens);
            expect(validator.exists('color.primary')).toBe(true);
        });
        it('应该验证颜色token', () => {
            const validator = (0, token_utils_1.createTokenValidator)(mockTokens);
            expect(validator.isColor('color.primary')).toBe(true);
        });
        it('应该验证尺寸token', () => {
            const validator = (0, token_utils_1.createTokenValidator)(mockTokens);
            expect(validator.isSize('spacing.xs')).toBe(true);
        });
        it('应该验证间距token', () => {
            const validator = (0, token_utils_1.createTokenValidator)(mockTokens);
            expect(validator.isSpacing('spacing.xs')).toBe(true);
        });
    });
    describe('transformColor', () => {
        it('应该转换HEX到RGB', () => {
            expect((0, token_utils_1.transformColor)('#ff0000', 'rgb')).toBe('rgb(255, 0, 0)');
        });
        it('应该转换HEX到HSL', () => {
            const result = (0, token_utils_1.transformColor)('#ff0000', 'hsl');
            expect(result).toMatch(/^hsl\(/);
        });
        it('应该保持HEX格式不变', () => {
            expect((0, token_utils_1.transformColor)('#ff0000', 'hex')).toBe('#ff0000');
        });
        it('应该处理oklch颜色', () => {
            const result = (0, token_utils_1.transformColor)('oklch(50%, 0.1, 0)', 'oklch');
            expect(result).toMatch(/^oklch\(/);
        });
    });
    describe('transformSize', () => {
        it('应该转换数字到px', () => {
            expect((0, token_utils_1.transformSize)(16, 'px')).toBe('16px');
        });
        it('应该转换数字到rem', () => {
            expect((0, token_utils_1.transformSize)(1, 'rem')).toBe('1rem');
        });
        it('应该转换px到rem', () => {
            expect((0, token_utils_1.transformSize)('16px', 'rem')).toBe('16rem');
        });
        it('应该保持相同单位不变', () => {
            expect((0, token_utils_1.transformSize)('16px', 'px')).toBe('16px');
        });
        it('应该处理纯数字字符串', () => {
            expect((0, token_utils_1.transformSize)('16', 'px')).toBe('16px');
        });
    });
    describe('transformSpacing', () => {
        it('应该转换单个值', () => {
            expect((0, token_utils_1.transformSpacing)('16', 'px')).toBe('16px');
        });
        it('应该转换多个值', () => {
            expect((0, token_utils_1.transformSpacing)('16 24', 'px')).toBe('16px 24px');
        });
        it('应该转换px值', () => {
            expect((0, token_utils_1.transformSpacing)('16px 24px', 'rem')).toBe('16rem 24rem');
        });
    });
    describe('createTokenTransformer', () => {
        it('应该创建token转换器', () => {
            const transformer = (0, token_utils_1.createTokenTransformer)(mockTokens);
            expect(transformer.transformColor('color.primary', 'hex')).toBe('#d45a5f');
        });
        it('应该转换尺寸', () => {
            const transformer = (0, token_utils_1.createTokenTransformer)(mockTokens);
            expect(transformer.transformSize('spacing.xs', 'rem')).toBe('4rem');
        });
        it('应该转换间距', () => {
            const transformer = (0, token_utils_1.createTokenTransformer)(mockTokens);
            expect(transformer.transformSpacing('spacing.xs', 'rem')).toBe('4rem');
        });
    });
    describe('createTokenCSS', () => {
        it('应该创建CSS变量名', () => {
            const css = (0, token_utils_1.createTokenCSS)(mockTokens);
            expect(css.createCSSVar('color.primary')).toBe('--yyc3-color-primary');
        });
        it('应该创建CSS属性', () => {
            const css = (0, token_utils_1.createTokenCSS)(mockTokens);
            const props = css.createCSSProperties(['color.primary', 'spacing.xs']);
            expect(props['--yyc3-color-primary']).toBe('#d45a5f');
        });
        it('应该支持自定义前缀', () => {
            const css = (0, token_utils_1.createTokenCSS)(mockTokens, 'custom');
            expect(css.createCSSVar('color.primary')).toBe('--custom-color-primary');
        });
    });
    describe('createTokenTheme', () => {
        it('应该创建主题', () => {
            const theme = (0, token_utils_1.createTokenTheme)(mockTokens);
            const overrides = { color: { primary: '#ff0000' } };
            const newTheme = theme.createTheme(mockTokens, overrides);
            expect(newTheme.color.primary).toBe('#ff0000');
        });
        it('应该合并多个主题', () => {
            const theme = (0, token_utils_1.createTokenTheme)(mockTokens);
            const overrides = [
                { color: { primary: '#ff0000' } },
                { spacing: { xs: '8px' } },
            ];
            const newTheme = theme.mergeThemes(...overrides);
            expect(newTheme.color.primary).toBe('#ff0000');
            expect(newTheme.spacing.xs).toBe('8px');
        });
    });
    describe('useToken', () => {
        it('应该创建token hook', () => {
            const token = (0, token_utils_1.useToken)(mockTokens);
            expect(token.get('color.primary')).toBe('#d45a5f');
