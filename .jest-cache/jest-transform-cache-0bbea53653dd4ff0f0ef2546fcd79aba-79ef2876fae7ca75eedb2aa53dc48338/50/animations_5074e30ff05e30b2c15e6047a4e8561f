const getAnimationString = (keyframe, config) => {
    const duration = config?.duration ? exports.animationDurations[config.duration] : '300ms';
    const easing = config?.easing ? exports.animationEasings[config.easing] : 'ease-in-out';
    const delay = config?.delay || '0ms';
    const fillMode = config?.fillMode || 'forwards';
    return `${keyframe} ${duration} ${easing} ${delay} ${fillMode}`;
};
exports.getAnimationString = getAnimationString;
