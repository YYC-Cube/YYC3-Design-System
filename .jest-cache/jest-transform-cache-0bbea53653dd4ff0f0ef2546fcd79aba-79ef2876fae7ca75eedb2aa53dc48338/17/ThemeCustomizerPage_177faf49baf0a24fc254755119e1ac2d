                        transition: `all var(--duration-fast) var(--easing-out)`
                      },
                      onMouseEnter: e => {
                        /* istanbul ignore next */
                        cov_1ghi3zp22e().f[128]++;
                        cov_1ghi3zp22e().s[323]++;
                        e.currentTarget.style.background = 'var(--destructive)';
                        /* istanbul ignore next */
                        cov_1ghi3zp22e().s[324]++;
                        e.currentTarget.style.color = 'var(--destructive-foreground)';
                      },
                      onMouseLeave: e => {
                        /* istanbul ignore next */
                        cov_1ghi3zp22e().f[129]++;
                        cov_1ghi3zp22e().s[325]++;
                        e.currentTarget.style.background = 'transparent';
                        /* istanbul ignore next */
                        cov_1ghi3zp22e().s[326]++;
                        e.currentTarget.style.color = '';
                      },
                      onFocus: e => {
                        /* istanbul ignore next */
                        cov_1ghi3zp22e().f[130]++;
                        cov_1ghi3zp22e().s[327]++;
                        e.currentTarget.style.outline = '2px solid var(--ring)';
                      },
                      onBlur: e => {
                        /* istanbul ignore next */
                        cov_1ghi3zp22e().f[131]++;
                        cov_1ghi3zp22e().s[328]++;
                        e.currentTarget.style.outline = '2px solid transparent';
                      },
                      "aria-label": t('themeCustomizer.removeFont'),
                      children: (0, jsx_runtime_1.jsx)(lucide_react_1.Trash2, {
                        className: "size-4",
                        "aria-hidden": "true"
                      })
                    })]
                  })]
                }, font.id);
              })
            }))
          })]
        }))]
      }), (0, jsx_runtime_1.jsx)("footer", {
        className: "rounded-xl border border-border",
        style: {
          padding: 'var(--spacing-4)',
          background: 'var(--card)',
          boxShadow: 'var(--shadow-sm)'
        },
        children: (0, jsx_runtime_1.jsxs)("div", {
          className: "flex flex-col sm:flex-row items-center justify-between gap-3",
          children: [(0, jsx_runtime_1.jsxs)("div", {
            className: "flex items-center gap-3",
            children: [(0, jsx_runtime_1.jsxs)("a", {
