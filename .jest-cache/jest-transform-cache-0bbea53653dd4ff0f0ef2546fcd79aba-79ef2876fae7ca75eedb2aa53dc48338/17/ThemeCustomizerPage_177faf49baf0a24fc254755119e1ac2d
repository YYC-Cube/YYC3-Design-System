function flattenTokens(obj, prefix) {
  /* istanbul ignore next */
  cov_1ghi3zp22e().f[9]++;
  const result =
  /* istanbul ignore next */
  (cov_1ghi3zp22e().s[37]++, {});
  /* istanbul ignore next */
  cov_1ghi3zp22e().s[38]++;
  for (const [key, value] of Object.entries(obj)) {
    const path =
    /* istanbul ignore next */
    (cov_1ghi3zp22e().s[39]++, prefix ?
    /* istanbul ignore next */
    (cov_1ghi3zp22e().b[4][0]++, `${prefix}-${key}`) :
    /* istanbul ignore next */
    (cov_1ghi3zp22e().b[4][1]++, key));
    /* istanbul ignore next */
    cov_1ghi3zp22e().s[40]++;
    if (typeof value === 'string') {
      /* istanbul ignore next */
      cov_1ghi3zp22e().b[5][0]++;
      cov_1ghi3zp22e().s[41]++;
      result[path] = value;
    } else {
      /* istanbul ignore next */
      cov_1ghi3zp22e().b[5][1]++;
      cov_1ghi3zp22e().s[42]++;
      if (
      /* istanbul ignore next */
      (cov_1ghi3zp22e().b[7][0]++, typeof value === 'object') &&
      /* istanbul ignore next */
      (cov_1ghi3zp22e().b[7][1]++, value !== null)) {
        /* istanbul ignore next */
        cov_1ghi3zp22e().b[6][0]++;
        cov_1ghi3zp22e().s[43]++;
        Object.assign(result, flattenTokens(value, path));
      } else
      /* istanbul ignore next */
      {
        cov_1ghi3zp22e().b[6][1]++;
      }
    }
  }
  /* istanbul ignore next */
  cov_1ghi3zp22e().s[44]++;
  return result;
}
function downloadFile(filename, content, mime) {
  /* istanbul ignore next */
  cov_1ghi3zp22e().f[10]++;
  const blob =
  /* istanbul ignore next */
