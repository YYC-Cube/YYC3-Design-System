/* 
   Utility: Compute relative luminance (sRGB)
    */
function hexToRgb(hex) {
  /* istanbul ignore next */
  cov_1ghi3zp22e().f[1]++;
  const h =
  /* istanbul ignore next */
  (cov_1ghi3zp22e().s[16]++, hex.replace('#', ''));
  /* istanbul ignore next */
  cov_1ghi3zp22e().s[17]++;
  return [parseInt(h.substring(0, 2), 16) / 255, parseInt(h.substring(2, 4), 16) / 255, parseInt(h.substring(4, 6), 16) / 255];
}
function luminance(hex) {
  /* istanbul ignore next */
  cov_1ghi3zp22e().f[2]++;
  const [r, g, b] =
  /* istanbul ignore next */
  (cov_1ghi3zp22e().s[18]++, hexToRgb(hex).map(c => {
    /* istanbul ignore next */
    cov_1ghi3zp22e().f[3]++;
    cov_1ghi3zp22e().s[19]++;
    return c <= 0.03928 ?
    /* istanbul ignore next */
    (cov_1ghi3zp22e().b[3][0]++, c / 12.92) :
    /* istanbul ignore next */
    (cov_1ghi3zp22e().b[3][1]++, Math.pow((c + 0.055) / 1.055, 2.4));
  }));
  /* istanbul ignore next */
  cov_1ghi3zp22e().s[20]++;
  return 0.2126 * r + 0.7152 * g + 0.0722 * b;
}
function contrastRatio(hex1, hex2) {
  /* istanbul ignore next */
  cov_1ghi3zp22e().f[4]++;
  const l1 =
  /* istanbul ignore next */
  (cov_1ghi3zp22e().s[21]++, luminance(hex1));
  const l2 =
  /* istanbul ignore next */
  (cov_1ghi3zp22e().s[22]++, luminance(hex2));
  const lighter =
  /* istanbul ignore next */
  (cov_1ghi3zp22e().s[23]++, Math.max(l1, l2));
  const darker =
  /* istanbul ignore next */
  (cov_1ghi3zp22e().s[24]++, Math.min(l1, l2));
  /* istanbul ignore next */
  cov_1ghi3zp22e().s[25]++;
  return (lighter + 0.05) / (darker + 0.05);
}
