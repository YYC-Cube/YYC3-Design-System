                outline: '2px solid transparent'
              },
              onFocus: e => {
                /* istanbul ignore next */
                cov_1ghi3zp22e().f[132]++;
                cov_1ghi3zp22e().s[329]++;
                e.currentTarget.style.outline = '2px solid var(--ring)';
              },
              onBlur: e => {
                /* istanbul ignore next */
                cov_1ghi3zp22e().f[133]++;
