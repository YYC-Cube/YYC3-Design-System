  /* istanbul ignore next */
  (cov_1ghi3zp22e().s[46]++, URL.createObjectURL(blob));
  const a =
  /* istanbul ignore next */
  (cov_1ghi3zp22e().s[47]++, document.createElement('a'));
  /* istanbul ignore next */
  cov_1ghi3zp22e().s[48]++;
  a.href = url;
  /* istanbul ignore next */
  cov_1ghi3zp22e().s[49]++;
  a.download = filename;
  /* istanbul ignore next */
  cov_1ghi3zp22e().s[50]++;
  a.click();
  /* istanbul ignore next */
  cov_1ghi3zp22e().s[51]++;
  URL.revokeObjectURL(url);
}
