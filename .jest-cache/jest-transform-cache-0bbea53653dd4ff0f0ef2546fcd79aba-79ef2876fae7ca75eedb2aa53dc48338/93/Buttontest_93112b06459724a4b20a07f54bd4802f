        const variants = ['default', 'destructive', 'outline', 'secondary', 'ghost', 'link'];
        variants.forEach((variant) => {
            const { unmount } = renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: variant, children: "Test" }));
            const button = dom_1.screen.getByText('Test');
            expect(button).toBeInTheDocument();
            unmount();
        });
    });
    it('应该正确处理所有 size 类型的样式返回', () => {
        const sizes = ['default', 'sm', 'lg', 'icon'];
        sizes.forEach((size) => {
            const { unmount } = renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { size: size, children: "Test" }));
            const button = dom_1.screen.getByText('Test');
            expect(button).toBeInTheDocument();
            unmount();
        });
    });
    it('应该正确处理 disabled 所有状态的组合', () => {
        const variants = ['default', 'destructive', 'outline', 'secondary', 'ghost', 'link'];
        variants.forEach((variant) => {
            const { unmount } = renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: variant, disabled: true, children: "Test" }));
            const button = dom_1.screen.getByText('Test');
            expect(button).toBeDisabled();
            unmount();
        });
    });
    it('应该正确处理 onClick 所有变体', () => {
        const handleClick = jest.fn();
        const variants = ['default', 'destructive', 'outline', 'secondary', 'ghost', 'link'];
        variants.forEach((variant) => {
            const { unmount } = renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: variant, onClick: handleClick, children: "Test" }));
            const button = dom_1.screen.getByText('Test');
            react_1.fireEvent.click(button);
            unmount();
        });
        expect(handleClick).toHaveBeenCalledTimes(6);
    });
    it('应该正确处理 variantStyles 变量的赋值', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: "default", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toBeInTheDocument();
    });
    it('应该正确处理 sizeStyles 的展开', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { size: "default", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            padding: '0.5rem 1rem',
        });
    });
    it('应该正确处理 radius 变量的使用', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            borderRadius: '0.5rem',
        });
    });
    it('应该正确处理所有 variant 的默认值回退', () => {
        const variants = ['default', 'destructive', 'outline', 'secondary', 'ghost', 'link'];
        variants.forEach((variant) => {
            const { unmount } = renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: variant, children: "Test" }));
            const button = dom_1.screen.getByText('Test');
            expect(button).toBeInTheDocument();
            unmount();
        });
    });
    it('应该正确处理所有 size 的默认值回退', () => {
        const sizes = ['default', 'sm', 'lg', 'icon'];
        sizes.forEach((size) => {
            const { unmount } = renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { size: size, children: "Test" }));
            const button = dom_1.screen.getByText('Test');
            expect(button).toBeInTheDocument();
            unmount();
        });
    });
    it('应该正确处理 link variant 的 padding 为 0', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: "link", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            padding: '0px',
        });
    });
    it('应该正确处理 outline variant 的边框条件', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: "outline", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            border: '1px solid rgb(224, 106, 112)',
        });
    });
    it('应该正确处理非 outline variant 的无边框', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: "default", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            background: 'rgb(224, 106, 112)',
            color: 'rgb(255, 255, 255)',
        });
    });
    it('应该正确处理 disabled 的 cursor 样式', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { disabled: true, children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            cursor: 'not-allowed',
        });
    });
    it('应该正确处理非 disabled 的 cursor 样式', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { disabled: false, children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            cursor: 'pointer',
        });
    });
    it('应该正确处理 link variant 不应用 sizeStyles', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: "link", size: "lg", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            padding: '0px',
        });
    });
    it('应该正确处理非 link variant 应用 sizeStyles', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: "default", size: "lg", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            padding: '0.75rem 1.5rem',
        });
    });
    it('应该正确处理 default size 的 padding', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { size: "default", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            padding: '0.5rem 1rem',
        });
    });
    it('应该正确处理 sm size 的 padding', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { size: "sm", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            padding: '0.25rem 0.75rem',
        });
    });
    it('应该正确处理 icon size 的 padding', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { size: "icon", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            padding: '0.5rem',
        });
    });
    it('应该正确处理 default size 的 fontSize', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { size: "default", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            fontSize: '1rem',
        });
    });
    it('应该正确处理 sm size 的 fontSize', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { size: "sm", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            fontSize: '0.875rem',
        });
    });
    it('应该正确处理 lg size 的 fontSize', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { size: "lg", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            fontSize: '1.5rem',
        });
    });
    it('应该正确处理 icon size 的 fontSize', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { size: "icon", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            fontSize: '1rem',
        });
    });
    it('应该正确处理所有 variant 的颜色值', () => {
        const { unmount } = renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: "default", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            background: 'rgb(224, 106, 112)',
            color: 'rgb(255, 255, 255)',
        });
        unmount();
        const { unmount: unmount2 } = renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: "destructive", children: "Test" }));
        const button2 = dom_1.screen.getByText('Test');
        expect(button2).toHaveStyle({
            background: 'rgb(255, 107, 91)',
            color: 'rgb(255, 255, 255)',
        });
        unmount2();
        const { unmount: unmount3 } = renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: "outline", children: "Test" }));
        const button3 = dom_1.screen.getByText('Test');
        expect(button3).toHaveStyle({
            background: 'transparent',
            color: 'rgb(224, 106, 112)',
        });
        unmount3();
        const { unmount: unmount4 } = renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: "secondary", children: "Test" }));
        const button4 = dom_1.screen.getByText('Test');
        expect(button4).toHaveStyle({
            background: 'rgb(45, 45, 45)',
            color: 'rgb(240, 240, 240)',
        });
        unmount4();
        const { unmount: unmount5 } = renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: "ghost", children: "Test" }));
        const button5 = dom_1.screen.getByText('Test');
        expect(button5).toHaveStyle({
            background: 'transparent',
            color: 'rgb(224, 106, 112)',
        });
        unmount5();
        const { unmount: unmount6 } = renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: "link", children: "Test" }));
        const button6 = dom_1.screen.getByText('Test');
        expect(button6).toHaveStyle({
            background: 'transparent',
            color: 'rgb(224, 106, 112)',
        });
        unmount6();
    });
    it('应该正确处理 link variant 的 textDecoration', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: "link", children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            textDecoration: 'underline',
        });
    });
    it('应该正确处理所有 variant 和 size 的组合', () => {
        const variants = ['default', 'destructive', 'outline', 'secondary', 'ghost', 'link'];
        const sizes = ['default', 'sm', 'lg', 'icon'];
        variants.forEach((variant) => {
            sizes.forEach((size) => {
                const { unmount } = renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { variant: variant, size: size, children: "Test" }));
                const button = dom_1.screen.getByText('Test');
                expect(button).toBeInTheDocument();
                unmount();
            });
        });
    });
    it('应该正确处理默认 variant 和默认 size', () => {
        renderWithTheme((0, jsx_runtime_1.jsx)(Button_1.Button, { children: "Test" }));
        const button = dom_1.screen.getByText('Test');
        expect(button).toHaveStyle({
            background: 'rgb(224, 106, 112)',
            color: 'rgb(255, 255, 255)',
            padding: '0.5rem 1rem',
        });
    });
});
